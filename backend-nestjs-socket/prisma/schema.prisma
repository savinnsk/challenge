

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
    id String @id @default(uuid())
    name String
    nickname String
    password String
    email String

    @@map("users")
}

model Room {
    id String @id @default(uuid())
    name String

     @@map("rooms")
}